
public class BeepPrintExample2  {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Runnable run=new BeepTesk();
		Thread thread=new Thread(run);
		thread.start();
		for(int i=0;i<5;i++){
			System.out.println("¶ò");
			try{
				Thread.sleep(500);
			}catch(Exception e){}
		}
	}

}
